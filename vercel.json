{
    "version": 2,
    "builds": [
      {
        "src": "rms/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "40.3mb", "runtime": "python3.9" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/staticfiles-build",
        "config": {
          "distDir": "staticfiless_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "rms/wsgi.py"
      }
    ]
  }